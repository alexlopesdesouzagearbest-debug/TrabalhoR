    {
      "cell_type": "code",
      "source": [
        "# ==== BLOCO 5: Impacto no total — Individual vs Equipe (edição recordista) ====\n",
        "library(dplyr)\n",
        "library(ggplot2)\n",
        "\n",
        "# 5.1) Agrega por Tipo\n",
        "impacto_tipo <- tabela_modalidade %>%\n",
        "  group_by(Tipo) %>%\n",
        "  summarise(Medalhas = sum(Medalhas), .groups = \"drop\") %>%\n",
        "  arrange(desc(Medalhas)) %>%\n",
        "  mutate(\n",
        "    Share = Medalhas / sum(Medalhas),\n",
        "    Label = paste0(Medalhas, \" (\", sprintf(\"%.1f%%\", 100 * Share), \")\"),\n",
        "    # garantir ordem de empilhamento (primeiro nível fica na base)\n",
        "    Tipo = factor(Tipo, levels = Tipo)\n",
        "  )\n",
        "\n",
        "# posição dos rótulos no meio de cada faixa\n",
        "impacto_tipo <- impacto_tipo %>%\n",
        "  mutate(Pos = cumsum(Medalhas) - Medalhas / 2)\n",
        "\n",
        "total_med <- sum(impacto_tipo$Medalhas)\n",
        "\n",
        "# 5.2) Stacked bar de 1 coluna (Total), com rótulos absoluto + %\n",
        "ggplot() +\n",
        "  geom_col(\n",
        "    data = impacto_tipo,\n",
        "    aes(x = \"Total\", y = Medalhas, fill = Tipo),\n",
        "    width = 0.5\n",
        "  ) +\n",
        "  geom_text(\n",
        "    data = impacto_tipo,\n",
        "    aes(x = \"Total\", y = Pos, label = Label),\n",
        "    fontface = \"bold\", size = 4\n",
        "  ) +\n",
        "  annotate(\n",
        "    \"text\",\n",
        "    x = \"Total\", y = total_med * 1.03,\n",
        "    label = paste0(\"Total: \", total_med),\n",
        "    fontface = \"bold\", size = 4\n",
        "  ) +\n",
        "  labs(\n",
        "    title = paste0(\"Impacto no total de medalhas — \", top_ed$Games[1], \" (\", top_ed$Year[1], \")\"),\n",
        "    subtitle = \"Equipe = 1 por evento; Individual = normal\",\n",
        "    x = NULL, y = \"Medalhas\", fill = NULL\n",
        "  ) +\n",
        "  theme_minimal(base_size = 12) +\n",
        "  theme(\n",
        "    legend.position = \"top\",\n",
        "    axis.text.x = element_blank(),\n",
        "    axis.ticks.x = element_blank()\n",
        "  )"
      ],
      "metadata": {
